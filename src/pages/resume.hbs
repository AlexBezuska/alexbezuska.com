{{>header}}





<div class="container">

  <div class="print-header">
    <div class="row">
      <div class="col-md-9">
        <span class="name">{{site.title}}</span><br><span class="tagline">{{site.subtitle}}</span>
      </div>
      <div class="col-md-3">
        <p>{{site.about.email}}<br>
          {{site.about.phone}}<br>
        New Albany, Indiana</p>
        </div>
      </div>
    </div>



    <div class="jumbotron no-padding-sm top-space bg-teal-gradient">
      <div class="container no-padding-sm">
        <div class="row no-margins">
          <div class="col-sm-4 col-xs-12">
            <img src="img/alex_bezuska_2022.png" class="alex-image" alt="{{site.title}} - {{site.subtitle}}"/>
          </div>
          <div class="col-sm-8 col-xs-12 pad1040">
            <h1 class="intro-heading">{{site.about.greeting}}</h1>
            <p class="hello-text">{{site.about.hello}}</p>
            <div class="hello-extended">

              {{#each site.about.hello-extended}}
              <p>{{{this}}}</p>
              {{/each}}

            </div>
          </div>
        </div>

      </div>
    </div>







    <div class="print-cover">
      <div class="row main-resume">
        <div class="col-md-12">
          <h2 class="resume-title top-space" id="workExperience">{{site.about.cover-letter.title}}</h2>

          <div class="crunch-width">

            {{#each site.about.cover-letter.body}}
            <p>{{{this}}}</p>
            {{/each}}


          <p>Thanks for your time,</p>
          <strong>{{site.about.name}}</strong>
        </div>
      </div>

    </div>
  </div>
</div>


<div class="container">
